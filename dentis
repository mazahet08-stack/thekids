<script>
  document.addEventListener("DOMContentLoaded", function () {
    const track = document.getElementById("winTickerTrack");
    if (!track) return;
    const clone = track.cloneNode(true);
    while (clone.firstChild) {
      track.appendChild(clone.firstChild);
    }
  });
</script>



